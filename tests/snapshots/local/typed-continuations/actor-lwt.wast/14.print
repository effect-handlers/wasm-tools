(module $scheduler
  (type $func (;0;) (func))
  (type $cont (;1;) (cont $func))
  (type (;2;) (func (param (ref $cont))))
  (type (;3;) (func (result i32)))
  (type (;4;) (func (result (ref null $cont))))
  (type (;5;) (func (result (ref $cont) (ref $cont))))
  (import "lwt" "yield" (tag (;0;) (type $func)))
  (import "lwt" "fork" (tag (;1;) (type 2) (param (ref $cont))))
  (import "queue" "queue-empty" (func $queue-empty (;0;) (type 3)))
  (import "queue" "dequeue" (func $dequeue (;1;) (type 4)))
  (import "queue" "enqueue" (func $enqueue (;2;) (type 2)))
  (func $run (;3;) (type 2) (param $main (ref $cont))
    local.get $main
    call $enqueue
    loop $l ;; label = @1
      call $queue-empty
      if ;; label = @2
        return
      end
      block $on_yield (result (ref $cont)) ;; label = @2
        block $on_fork (type 5) (result (ref $cont) (ref $cont)) ;; label = @3
          call $dequeue
          resume $cont (tag 0 1 (;@2;))(tag 1 0 (;@3;))
          br 2 (;@1;)
        end
        call $enqueue
        call $enqueue
        br 1 (;@1;)
      end
      call $enqueue
      br 0 (;@1;)
    end
  )
  (export "run" (func $run))
)